//O(n*lgn)
#include<iostream>
#include<cstdio>
#include<cstring>
#include<cstdlib>
#include<cmath>
#include<ctime>
#include<algorithm>
using namespace std;
const int INF=1e9;
const int MAXN=40010;
int d[MAXN],a[MAXN];
int main()
{
  int ans,i,j,va,T,n;
  scanf("%d",&T);
  while(T--)
  {
    scanf("%d",&n);
    ans=0;
    d[0]=-INF;
    for(i=1;i<=n;++i)
    {
      scanf("%d",&a[i]);
      if(d[ans]<a[i])
      {
       d[++ans]=a[i];
      }
      else
      {
        j=lower_bound(d+1,d+ans,a[i])-d-1;//STL查找一个数列中小于a[i]的最后一个数
                                         //这里用到了单调性d[1]<d[2]<d[3]....
        d[j+1]=a[i];
      }
    }
    printf("%d\n",ans);
  }
  return 0;
}
