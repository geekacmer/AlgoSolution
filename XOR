#include<cstdio>
#include<iostream>
#include<cstring>
#include<cstdlib>
#include<cmath>
#include<ctime>
#include<algorithm>
using namespace std;
const int MAXN=1e5+10;
int a[MAXN],b[MAXN];
//int cmp(int a,int b)
//{
 // return a<b;
//}
int main()
{
  int n,i;
  int ans=0;
  scanf("%d",&n);
  for(i=0;i<n;++i)
  {
    scanf("%d",&a[i]);
    ans^=a[i];
  }
  for(i=0;i<n;++i)
  {
    scanf("%d",&b[i]);
    ans^=b[i];
  }
   for(i=0;i<n;++i)
    a[i]^=ans;
   sort(a,a+n),sort(b,b+n);
   for(i=0;i<n;++i)
    if(a[i]!=b[i])
     break;
   if(i==n)
    printf("%d\n",ans);
   else
    printf("-1\n");
   return 0;
}
